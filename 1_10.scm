(define (A x y)
  (cond ((= y 0) 0)
        ((= x 0) (* 2 y))
        ((= y 1) 2)
        (else (A (- x 1)
                 (A x (- y 1))))))

(A 1 10) ;; 1024
(A 2 4) ;; 65536
(A 3 3) ;; 65536

(define (f n) (A 0 n))

;; | n  | fn |
;; | 0  | 0  |
;; | 1  | 2  |
;; | 2  | 4  |
;; | 3  | 6  |
;; | 4  | 8  |
;; | 5  | 10 |
;; | 6  | 12 |
;; | 7  | 14 |
;; | 8  | 16 |
;; | 9  | 18 |
;; | 10 | 20 |

;; => f(n) = 2*n

(define (g n) (A 1 n))

;; | n  | gn   |
;; | 0  | 0    |
;; | 1  | 2    |
;; | 2  | 4    |
;; | 3  | 8    |
;; | 4  | 16   |
;; | 5  | 32   |
;; | 6  | 64   |
;; | 7  | 128  |
;; | 8  | 256  |
;; | 9  | 512  |
;; | 10 | 1024 |

;; => g(n) = 2^n

(define (h n) (A 2 n))

;; | n  | hn    |
;; | 0  | 0     |
;; | 1  | 2     |
;; | 2  | 4     |
;; | 3  | 16    |
;; | 4  | 65536 |
;; | 5  | ...   |
;; | 6  | ...   |
;; | 7  | ...   |
;; | 8  | ...   |
;; | 9  | ...   |
;; | 10 | ...   |

;; => h(n) = 2^2^..2 ;; 连续求n次2次幂

